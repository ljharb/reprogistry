{
	"name": "reprogistry",
	"private": true,
	"version": "0.0.0",
	"description": "registry of reproduction results from the npm registry",
	"main": "index.js",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"lint": "eslint .",
		"postlint": "tsc",
		"pretest": "npm run lint",
		"tests-only": "tape 'test/**/*.mjs'",
		"test": "npm run tests-only",
		"preversion": "echo \"use \\`npm run --no-workspaces version\\` in a subpackage directly\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/reprogistry/reprogistry.git"
	},
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/reprogistry/reprogistry/issues"
	},
	"homepage": "https://github.com/reprogistry/reprogistry#readme",
	"dependencies": {
		"pacote": "^21.0.4",
		"reproduce": "^1.2.0",
		"semver": "^7.7.3",
		"validate-npm-package-name": "^7.0.2"
	},
	"devDependencies": {
		"@actions/core": "^2.0.2",
		"@ljharb/eslint-config": "^22.1.3",
		"@ljharb/tsconfig": "^0.3.2",
		"@types/node": "^22.19.7",
		"@types/pacote": "^11.1.8",
		"@types/semver": "^7.7.1",
		"eslint": "^9.39.2",
		"tape": "^5.9.0",
		"typescript": "next"
	},
	"overrides": {
		"reproduce": {
			"@vltpkg/package-info": "0.0.0-0.1730724342581"
		}
	}
}
