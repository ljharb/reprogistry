"^4.0.0\",\n+\t\t\"@types/jest\": \"^28.1.6\",\n+\t\t\"@typescript-eslint/eslint-plugin\": \"^5.30.7\",\n+\t\t\"@typescript-eslint/parser\": \"^5.30.7\",\n+\t\t\"eslint\": \"^8.20.0\",\n+\t\t\"eslint-config-base-web3\": \"0.1.0\",\n+\t\t\"eslint-config-prettier\": \"^8.5.0\",\n+\t\t\"eslint-plugin-import\": \"^2.26.0\",\n+\t\t\"flatted\": \"^3.3.1\",\n+\t\t\"jest\": \"^29.7.0\",\n+\t\t\"jest-extended\": \"^3.0.1\",\n",
